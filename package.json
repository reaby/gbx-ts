	},
	"optionalDependencies": {
		"lzo-ts": "^0.1.1"
	},
	"homepage": "https://github.com/ThaumicTom/gbx.js#readme",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/ThaumicTom/gbx.js.git"
	},
	"bugs": {
		"web": "https://github.com/ThaumicTom/gbx.js/issues"
	},
	"publishConfig": {
		"registry": "https://registry.npmjs.org/"
